<input id="@Id"
    data-provide="slider"
    data-slider-id="@(Id)-render"
    data-slider-max="@DamageTrack.Total"
    data-slider-step="1"
    data-slider-value="@DamageTrack.Current"
    data-slider-rangeHighlights='[{ "start": @DamageTrack.Boxes, "end": @DamageTrack.Total, "class": "progress-bar progress-bar-striped bg-danger" }]'  
/>